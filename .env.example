
# Suno.ai account credentials
# Option 1: Use email/password authentication
EMAIL=your-email@example.com
PASSWORD=your-password

# Option 2: Use Chrome profile (set to "True" to enable)
USE_CHROME_PROFILE=False
# Path to Chrome user data directory (required if USE_CHROME_PROFILE=True)
CHROME_USER_DATA_DIR=C:\Users\YourUsername\AppData\Local\Google\Chrome\User Data

# Optional settings
# Set to "True" to run in headless mode (no browser UI)
HEADLESS=False

# Custom download path (default is user's Downloads folder)
# DOWNLOAD_PATH=C:\Users\YourName\Music\SunoDownloads
